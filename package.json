{
  "name": "wrapup",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "start:api": "nx serve api",
    "build": "nx build",
    "svg": "svg-generator",
    "test": "nx test",
    "test:watch": "nx test --watch",
    "test:calendar:watch": "nx test calendar --watch",
    "test:api:unit": "nx test api --test-path-pattern=src/app/*",
    "test:api:integration": "nx test api --test-path-pattern=tests/*"
  },
  "svgGenerator": {
    "outputPath": "apps/wrapup/src/app/svg",
    "srcPath": "apps/wrapup/src/assets/svg",
    "svgoConfig": {
      "plugins": [
        "removeDimensions",
        "cleanupAttrs",
        {
          "name": "convertColors",
          "params": {
            "currentColor": true
          }
        }
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.8",
