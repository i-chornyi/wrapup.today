    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@ngneat/svg-icon": "^7.0.0",
    "@nrwl/angular": "15.9.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/uuid": "^9.0.0",
    "autolinker": "^4.0.0",
    "axios": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
