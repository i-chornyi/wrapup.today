{
  "name": "wrapup",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve",
    "start:api": "nx serve api",
    "build:api": "nx build api",
    "run:api": "node ./dist/apps/api/main.js",
    "run:frontend": "node ./dist/apps/wrapup/main.js",
    "build": "nx build",
    "svg": "svg-generator",
    "test": "nx test",
    "test:e2e": "nx e2e wrapup-e2e --headed --trace=on",
    "test:api:unit": "nx test api --test-path-pattern=src/app/*",
    "test:api:e2e": "nx test api --test-path-pattern=tests/*",
    "test:calendar": "nx test calendar",
    "test-seeder:build": "nx run data-seeder:build",
    "test-seeder:run": "node ./dist/apps/data-seeder/main.js",
    "test-seeder": "npm run test-seeder:build && npm run test-seeder:run --"
  },
  "svgGenerator": {
    "outputPath": "apps/wrapup/src/app/svg",
    "srcPath": "apps/wrapup/src/assets/svg",
    "svgoConfig": {
      "plugins": [
        "removeDimensions",
        "cleanupAttrs",
        {
          "name": "convertColors",
          "params": {
            "currentColor": true
          }
        }
      ]
    }
  },
  "private": true,
  "devDependencies": {
